{
    "name": "k-monitor",
    "version": "3.2.1",
    "description": "Express '/monitor' dashboard and server metrics/health for Node.js",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts && mkdir -p dist/public && cp -r public/* dist/public/ 2>/dev/null || true",
        "dev": "tsx src/dev.ts",
        "prepare": "npm run build && npm run obfuscate",
        "obfuscate": "node scripts/obfuscate.mjs"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.14.2",
        "tsup": "^8.0.2",
        "tsx": "^4.16.2",
        "javascript-obfuscator": "^4.0.2",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "express": ">=4 <6"
    }
}